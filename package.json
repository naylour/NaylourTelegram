{
    "name": "@naylour/telegram",
    "module": "./dist/index.js",
    "version": "1.0.0",
    "types": "./dist/index.d.ts",
    "private": false,
    "type": "module",
    "author": {
        "email": "ShahTheShah@ya.ru",
        "name": "Naylour",
        "url": "https://github.com/naylour"
    },
    "repository": {
        "type": "git",
        "directory": "/",
        "url": "https://github.com/naylour/NaylourTelegram.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/naylour/NaylourTelegram/issues",
        "email": "ShahTheShah@ya.ru"
    },
    "categories": ["Other"],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./types": {
            "types": "./dist/Telegram.d.ts",
            "default": "./dist/Telegram.d.ts"
        },
        "./reactivity": {
            "import": "./dist/reactivity.svelte.js",
            "types": "./dist/reactivity.svelte.d.ts",
            "default": "./dist/reactivity.svelte.js"
        }
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/node": "^22.10.3",
        "terser": "^5.37.0",
        "vite": "^6.0.6",
        "vite-plugin-dts": "^4.4.0"
    },
    "peerDependencies": {
        "typescript": "^5.0.0",
        "svelte": "^5.16.0"
    },
    "dependencies": {
        "esm-env": "^1.2.1"
    },
    "scripts": {
        "package": "bun --bun vite build",
        "publicate": "bun publish --access public"
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "keywords": [
        "telegram",
        "telegram-api",
        "svelte",
        "svelte-5"
    ],
    "pricing": "Free"
}
